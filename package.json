    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/cz-commitlint": "^17.3.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "chalk": "4",
    "commitizen": "^4.2.6",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.30.0",
    "execa": "5",
    "husky": "^8.0.2",
    "inquirer": "8",
    "lint-staged": "^13.1.0",
    "minimist": "^1.2.7",
    "semver": "^7.3.8",
